@inherits SkyBlazorComponentBase

<div class="movein-popover-block" @onmouseenter="@MouseMoveIn" @onmouseleave="@MouseMoveOut" @onclick="@_mouseClickEvent">
	@if (MoveInContent is null)
	{
		<div class="default-head-block">
			<i class="fa fa-user fa-2x"></i>
		</div>
	}
	else
	{
		@MoveInContent
	}

	<div @attributes="@AdditionalAttributes" class="@ClassCssValue">
		<div class="d-flex user-define-block">
			@PopoverContent
		</div>
	</div>
</div>
